{
  "expo": {
    "name": "neuro-nudge",
    "slug": "neuro-nudge-v2",
    "version": "2.1.2",
    "orientation": "portrait",
    "icon": "./assets/Neuro-Nudge_Logo_Blue.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Neuro-Nudge_Logo_Blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#217180"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neuronudge.neuronudge",
      "buildNumber": "2",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSPhotoLibraryUsageDescription": "This app does not access your photos.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.neuronudge.neuronudge",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Neuro-Nudge_Logo_Blue.png",
        "backgroundColor": "#217180"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/Neuro-Nudge_Logo_Blue.png",
          "color": "#217180"
        }
      ],
      "@react-native-community/datetimepicker",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "f82ccb32-f09c-4330-b911-0c84a5623f55"
      }
    },
    "owner": "jonnymcfadden1528s-organization",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f82ccb32-f09c-4330-b911-0c84a5623f55",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000,
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    }
  }
}
